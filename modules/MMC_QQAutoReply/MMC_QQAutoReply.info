{
"id": "MMC_QQAutoReply",
"cate":"MMC模块",
"name":"QQ-MMC-监控自动回复",
"url": "http://www.bejson.com",
"version": "1.0",
"code":"QQ-MMC-AR",
"icon": "vcard-o",
"remark":"QQ-MMC-监控自动回复",
"setting":[
	{
    "id": "repo_material_cate_id",
    "name":"验证信息素材库",
    "type":"repo",
    "source":"material",
    "top_cate_id":"12",
    "remark":"取验证语"
    },

    {
	"id": "add_count",
	"name":"回复好友的次数",
	"type":"textbox",
	"defaultValue":"5",
	"remark":"单位：个"
	},
	{
	"id": "sendTime",
	"name":"发消息的间隔(随机)",
	"type":"textbox",
	"defaultValue":"3-5",
	"remark":"单位：秒"
	},
	{
	"id": "meg_count",
	"name":"每次回复好友发消息的个数(随机)",
	"type":"textbox",
	"defaultValue":"5",
	"remark":"单位：个"
	},

    {
    "id": "time_delay",
    "name":"延时",
    "type":"textbox",
    "defaultValue":"3",
    "remark":"单位：秒"
    },
    {
    "id": "set_timeStart",
    "name":"在随机范围时间只运行一次(始)",
    "type":"textbox",
    "defaultValue":"60",
    "remark":"单位：分钟"
    },
    {
    "id": "set_timeEnd",
    "name":"在随机范围时间只运行一次(终)",
    "type":"textbox",
    "defaultValue":"120",
    "remark":"单位：分钟"
    },
    {
    "id": "startTime",
    "name":"模块不运行的开始时间",
    "type":"textbox",
    "defaultValue":"0",
    "remark":"格式如0:00或0(24小时制)"
    },
    {
    "id": "endTime",
    "name":"模块不运行的结束时间",
    "type":"textbox",
    "defaultValue":"8",
    "remark":"格式如8:00或8(24小时制)"
    }
]
}
