{
"id": "QQEmailSendTextAll",
"cate":"其他模块",
"name":"QQ邮箱根据检测收信情况发信",
"url": "http://www.bejson.com",
"version": "1.0",
"code":"QQEmailSendTextAll",
"icon":"vcard-o",
"remark":"QQ邮箱根据检测收信情况发信",
"setting":[
	{
	"id": "repo_material_cateId",
	"name":"邮件内容仓库",
	"type":"repo",
	"source":"material",
	"top_cate_id":"12"
	},
	{
	"id": "repo_material_cateId2",
	"name":"邮件主题仓库",
	"type":"repo",
	"source":"material",
	"top_cate_id":"12"
	},
	{
	"id": "repo_mail_cateId",
	"name":"普通号码库",
	"type":"repo",
	"source":"number",
	"top_cate_id":"22"
	},
	{
	"id": "auto_reply_cate_id",
	"name":"自动回复号码库",
	"type":"repo",
	"source":"number",
	"top_cate_id":"22"
	},
	{
	"id": "train_cate_id",
	"name":"小号号码库",
	"type":"repo",
	"source":"number",
	"top_cate_id":"22"
	},
	{
	"id": "nuberLoop",
	"name":"是否循环号码库",
	"type":"select",
	"list":{"循环":"循环","不循环":"不循环"},
	"defaultValue":"不循环",
	"remark":"是否循环号码库"
	},
	{
	"id": "addToText",
	"name":"是否添加到正文",
	"type":"select",
	"list":{"添加":"添加","不添加":"不添加"},
	"defaultValue":"添加",
	"remark":"图片是否添加到正文"
	},
	{
	"id": "selectContent",
	"name":"发送主题或内容",
	"type":"select",
	"list":{"只发主题":"只发主题","只发内容":"只发内容","主题内容都发":"主题内容都发"},
	"defaultValue":"主题内容都发",
	"remark":"发送主题或内容"
	},
	{
	"id": "size",
	"name":"图片大小",
	"type":"select",
	"list":{"大":"大","小":"小","中":"中","实际大小":"实际大小"},
	"defaultValue":"大",
	"remark":"图片大小"
	},
    {
	"id": "sendCount",
	"name":"一个帐号发邮件次数",
	"type":"textbox",
	"defaultValue":"3",
	"remark":"单位：次"
	},
	{
	"id": "failCount",
	"name":"待发送个数",
	"type":"textbox",
	"defaultValue":"4",
	"remark":"当待发送个数达到此个数时，跳出模块"
	},
	{
	"id": "random_code",
	"name":"乱码专用",
	"type":"select",
	"list":{"乱码":"乱码","不乱码":"不乱码"},
	"defaultValue":"不乱码",
	"remark":"出现乱码可修改该设置"
	},
    {
    "id": "time_delay",
    "name":"等待时间",
    "type":"textbox",
    "defaultValue":"3",
    "remark":"发信后与进收件箱之间的等待时间"
    }
]
}
