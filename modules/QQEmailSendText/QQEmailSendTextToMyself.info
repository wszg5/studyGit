{
"id": "QQEmailSendTextToMyself",
"cate":"其他模块",
"name":"QQ邮箱发消息(对自己)",
"url": "http://www.bejson.com",
"version": "1.0",
"code":"QQEmailSendTextToMyself",
"icon":"vcard-o",
"remark":"QQ邮箱发消息(对自己)",
"setting":[
	{
	"id": "repo_material_cateId",
	"name":"邮件内容仓库",
	"type":"repo",
	"source":"material",
	"top_cate_id":"12"
	},
	{
	"id": "repo_material_cateId2",
	"name":"邮件主题仓库",
	"type":"repo",
	"source":"material",
	"top_cate_id":"12"
	},
	{
	"id": "picture",
	"name":"是否发送图片",
	"type":"select",
	"list":{"是":"是","否":"否"},
	"defaultValue":"是",
	"remark":"是否发送图片"
	},
	{
	"id": "selectContent",
	"name":"发送主题或内容",
	"type":"select",
	"list":{"只发主题":"只发主题","只发内容":"只发内容","主题内容都发":"主题内容都发"},
	"defaultValue":"主题内容都发",
	"remark":"发送主题或内容"
	},
	{
	"id": "size",
	"name":"图片大小",
	"type":"select",
	"list":{"大":"大","小":"小","中":"中","实际大小":"实际大小"},
	"defaultValue":"大",
	"remark":"图片大小"
	},
    {
	"id": "sendCount",
	"name":"发邮件随机次数",
	"type":"textbox",
	"defaultValue":"1-2",
	"remark":"单位：次"
	},
	{
    "id": "random_code",
    "name":"乱码专用",
    "type":"select",
    "list":{"乱码":"乱码","不乱码":"不乱码"},
    "defaultValue":"不乱码",
    "remark":"出现乱码可修改该设置"
    },
    {
    "id": "time_delay",
    "name":"延时",
    "type":"textbox",
    "defaultValue":"3",
    "remark":"单位：秒"
    }
]
}
