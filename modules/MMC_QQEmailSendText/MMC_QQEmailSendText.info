{
"id": "MMC_QQEmailSendText",
"cate":"MMC模块",
"name":"QQEmail-MMC-邮件发消息",
"url": "http://www.bejson.com",
"version": "1.0",
"code":"QQEmail-MMC-01",
"icon":"vcard-o",
"remark":"QQEmail-MMC-邮件发消息",
"setting":[
    {
    "id": "set_timeStart",
    "name":"在随机范围时间只运行一次(始)",
    "type":"textbox",
    "defaultValue":"60",
    "remark":"单位：分钟"
    },
    {
    "id": "set_timeEnd",
    "name":"在随机范围时间只运行一次(终)",
    "type":"textbox",
    "defaultValue":"120",
    "remark":"单位：分钟"
    },
    {
    "id": "startTime",
    "name":"模块不运行的开始时间",
    "type":"textbox",
    "defaultValue":"0",
    "remark":"格式如0:00或0(24小时制)"
    },
    {
    "id": "endTime",
    "name":"模块不运行的结束时间",
    "type":"textbox",
    "defaultValue":"8",
    "remark":"格式如8:00或8(24小时制)"
    }
	{
	"id": "repo_material_cateId",
	"name":"邮件内容仓库",
	"type":"repo",
	"source":"material",
	"top_cate_id":"12"
	},
	{
	"id": "repo_material_cateId2",
	"name":"邮件主题仓库",
	"type":"repo",
	"source":"material",
	"top_cate_id":"12"
	},
	{
	"id": "repo_mail_cateId",
	"name":"收件人号码仓库",
	"type":"repo",
	"source":"number",
	"top_cate_id":"22"
	},
	{
	"id": "picture",
	"name":"是否发送图片",
	"type":"select",
	"list":{"是":"是","否":"否"},
	"defaultValue":"是",
	"remark":"是否发送图片"
	},
	{
	"id": "selectContent",
	"name":"选择发送形式",
	"type":"select",
	"list":{"只发内容":"只发内容","只发主题":"只发主题","主题内容都发":"主题内容都发"},
	"defaultValue":"只发内容",
	"remark":"选择发送形式"
	},
	{
	"id": "emailType",
	"name":"邮件类型",
	"type":"select",
	"list":{"QQ邮箱":"QQ邮箱","189邮箱":"189邮箱"},
	"defaultValue":"QQ邮箱",
	"remark":"邮件类型"
	},
	{
	"id": "size",
	"name":"图片大小",
	"type":"select",
	"list":{"大":"大","小":"小","中":"中"},
	"defaultValue":"小",
	"remark":"图片大小"
	},
	{
	"id": "count",
	"name":"发邮件总人数",
	"type":"textbox",
	"defaultValue":"4",
	"remark":"单位：个"
	},

    {
    "id": "time_delay",
    "name":"唤醒拉起休眠时间",
    "type":"textbox",
    "defaultValue":"8",
    "remark":"单位：秒"
    }
]
}
