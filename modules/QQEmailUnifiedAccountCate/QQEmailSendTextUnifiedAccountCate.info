{
"id": "QQEmailSendTextUnifiedAccountCate",
"cate":"其他模块",
"name":"QQ邮箱发消息(指定服务器版)",
"url": "http://www.bejson.com",
"version": "1.0",
"code":"QQEmailSendTextUnifiedAccountCate",
"icon":"vcard-o",
"remark":"QQ邮箱发消息",
"setting":[
{
    "id": "domain",
    "name":"指定服务器",
    "type":"textbox",
    "defaultValue":"http://192.168.1.108:8888",
    "remark":"格式如：http://192.168.1.108:8888"
    },
	{
    "id": "repo_material_cateId",
    "name":"对应服务器上的邮件内容仓库",
    "defaultValue":"000",
    "type":"textbox",
    },
    {
    "id": "repo_material_cateId2",
    "name":"对应服务器上的邮件主题仓库",
    "defaultValue":"000",
    "type":"textbox",
    },
	{
	"id": "repo_mail_cateId",
	"name":"号码库分类",
	"type":"repo",
	"source":"number",
	"top_cate_id":"22"
	},
	{
	"id": "picture",
	"name":"是否发送图片",
	"type":"select",
	"list":{"是":"是","否":"否"},
	"defaultValue":"是",
	"remark":"是否发送图片"
	},
	{
	"id": "nuberLoop",
	"name":"是否循环号码库",
	"type":"select",
	"list":{"循环":"循环","不循环":"不循环"},
	"defaultValue":"不循环",
	"remark":"是否循环号码库"
	},
	{
	"id": "addToText",
	"name":"是否添加到正文",
	"type":"select",
	"list":{"添加":"添加","不添加":"不添加"},
	"defaultValue":"添加",
	"remark":"图片是否添加到正文"
	},
	{
	"id": "selectContent",
	"name":"发送主题或内容",
	"type":"select",
	"list":{"只发主题":"只发主题","只发内容":"只发内容","主题内容都发":"主题内容都发"},
	"defaultValue":"主题内容都发",
	"remark":"发送主题或内容"
	},
	{
	"id": "size",
	"name":"图片大小",
	"type":"select",
	"list":{"大":"大","小":"小","中":"中","实际大小":"实际大小"},
	"defaultValue":"大",
	"remark":"图片大小"
	},
	{
	"id": "count",
	"name":"每次发邮件收件人个数",
	"type":"textbox",
	"defaultValue":"1",
	"remark":"单位：个"
	},
	{
	"id": "bccCount",
	"name":"每次发邮件密送人数",
	"type":"textbox",
	"defaultValue":"0",
	"remark":"单位：个"
	},
    {
	"id": "sendCount",
	"name":"一个帐号发邮件次数",
	"type":"textbox",
	"defaultValue":"3",
	"remark":"单位：次"
	},
	{
	"id": "failCount",
	"name":"待发送个数",
	"type":"textbox",
	"defaultValue":"4",
	"remark":"当待发送个数达到此个数时，跳出模块"
	},
	{
	"id": "deleteCont",
	"name":"每发多少个邮件去删信",
	"type":"textbox",
	"defaultValue":"10",
	"remark":"每发多少个邮件去执行删除已发送和已删除里面的邮件"
	},
	{
	"id": "random_code",
	"name":"乱码专用",
	"type":"select",
	"list":{"乱码":"乱码","不乱码":"不乱码"},
	"defaultValue":"不乱码",
	"remark":"出现乱码可修改该设置"
	},
    {
    "id": "time_delay",
    "name":"延时",
    "type":"textbox",
    "defaultValue":"3",
    "remark":"单位：秒"
    }
]
}
